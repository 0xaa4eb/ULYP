<?xml version="1.0" encoding="UTF-8"?>

<?scenebuilder-stylesheet DarkTheme.css?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>


<?import javafx.scene.control.Label?>
<VBox maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
      minWidth="-Infinity" prefHeight="400.0" prefWidth="600.0"
      xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="com.ulyp.ui.SettingsView">
   <children>
      <HBox prefWidth="200.0">
         <children>
            <Label prefWidth="200.0" text="Theme" />
            <ChoiceBox fx:id="themeChoiceBox" prefWidth="150.0" />
         </children>
      </HBox>
      <HBox prefWidth="200.0">
         <children>
            <Label prefWidth="200.0" text="Font" />
            <ChoiceBox fx:id="fontChoiceBox" prefWidth="150.0" />
         </children>
      </HBox>
      <Button mnemonicParsing="false" text="Ok">
         <VBox.margin>
            <Insets bottom="10.0" left="20.0" right="20.0" top="10.0" />
         </VBox.margin>
      </Button>
   </children>
</VBox>
